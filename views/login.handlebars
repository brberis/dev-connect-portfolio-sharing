<section class="container ">
  <div class="row pt-3">
    <div class="col-6">
      <h2>Login</h2>
      <hr />
      <form class="login-form">
        <div class="form-group">
          <label for="email-login">Email:</label>
          <input class="form-control" type="text" id="email-login" />
        </div>
        <div class="form-group">
          <label for="password-login">Password:</label>
          <input class="form-control" type="password" id="password-login" />
        </div>
        <div class="form-group">
          <button class="btn" type="submit">Login</button>
        </div>
        <div>
          <p>
            Do not have an account? <a href="/signup" role="button" class="text-primary font-weight-bold">Sign-up Here!</a>
          </p>
        </div>
      </form>
    </div>
  </div>
</section>

